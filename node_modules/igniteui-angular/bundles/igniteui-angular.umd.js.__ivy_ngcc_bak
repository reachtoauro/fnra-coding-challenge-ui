            get: function () {
                return this.focusState;
            },
            /**
             * Sets the state of the igxFocus.
             * ```html
             * <igx-input-group >
             *  <input #focusContainer igxInput [igxFocus]="true"/>
             * </igx-input-group>
             * ```
             * @memberof IgxFocusDirective
