                case DisplayDensity.cosy:
                    return 'igx-paginator--cosy';
                case DisplayDensity.compact:
                    return 'igx-paginator--compact';
                default:
                    return 'igx-paginator';
            }
        };
        Object.defineProperty(IgxGridBaseDirective.prototype, "pinnedWidth", {
            /**
