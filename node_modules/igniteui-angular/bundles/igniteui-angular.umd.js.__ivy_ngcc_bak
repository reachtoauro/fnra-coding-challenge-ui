            get: function () {
                var pId = this.parentId ? this.parentId.substring(this.parentId.indexOf(this.layout_id) + this.layout_id.length) + '-' : '';
                return this.layout_id + pId + this.key;
            },
            enumerable: false,
            configurable: true
        });
