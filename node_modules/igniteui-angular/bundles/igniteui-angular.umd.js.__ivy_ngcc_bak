    // ================================= IgxTabPanelComponent ======================================
    var IgxTabPanelComponent = /** @class */ (function () {
        function IgxTabPanelComponent(_tabBar, _element) {
            this._tabBar = _tabBar;
            this._element = _element;
            /**
             * @hidden
