             * ```
             * @memberof IgxGridCellComponent
             */
            get: function () {
                return this.gridAPI.grid;
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(IgxGridCellComponent.prototype, "rowIndex", {
