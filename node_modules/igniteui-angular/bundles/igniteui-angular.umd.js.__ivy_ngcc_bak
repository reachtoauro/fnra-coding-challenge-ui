        IgxRadioComponent.prototype.onFocus = function () {
            this.focused = true;
        };
        /**
         * @hidden
         */
        IgxRadioComponent.prototype.onBlur = function () {
            this.focused = false;
            this._onTouchedCallback();
        };
        /**
         * @hidden
         */
        IgxRadioComponent.prototype.registerOnChange = function (fn) { this._onChangeCallback = fn; };
        /**
