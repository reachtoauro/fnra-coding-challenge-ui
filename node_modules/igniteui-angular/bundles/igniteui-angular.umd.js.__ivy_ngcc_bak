                switch (this.labelPosition) {
                    case RadioLabelPosition.BEFORE:
                        return this.cssClass + "__label--before";
                    case RadioLabelPosition.AFTER:
                    default:
                        return this.cssClass + "__label";
                }
            },
            enumerable: false,
            configurable: true
        });
        /**
         * @hidden
