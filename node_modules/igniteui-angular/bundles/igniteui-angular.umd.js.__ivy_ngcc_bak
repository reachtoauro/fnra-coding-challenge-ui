                fieldToDeselect = columns;
            }
            else {
                columns.forEach(function (col) {
                    if (col.columnGroup) {
                        var children = col.allChildren.filter(function (c) { return !c.columnGroup; }).map(function (c) { return c.field; });
                        fieldToDeselect = __spread(fieldToDeselect, children);
                    }
                    else {
