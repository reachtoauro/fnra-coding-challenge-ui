            get: function () {
                if (this._tabBar.tabs) {
                    return this._tabBar.tabs.toArray()[this.index];
                }
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(IgxTabPanelComponent.prototype, "changesCount", {
