    var DropPosition;
    (function (DropPosition) {
        DropPosition[DropPosition["BeforeDropTarget"] = 0] = "BeforeDropTarget";
        DropPosition[DropPosition["AfterDropTarget"] = 1] = "AfterDropTarget";
