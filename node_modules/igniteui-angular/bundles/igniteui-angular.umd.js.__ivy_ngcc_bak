    /** @hidden */
    var IGX_TIME_PICKER_COMPONENT = 'IgxTimePickerComponentToken';
    /** @hidden */
    var TimeParts;
    (function (TimeParts) {
        TimeParts["Hour"] = "hour";
        TimeParts["Minute"] = "minute";
        TimeParts["Seconds"] = "seconds";
        TimeParts["amPM"] = "ampm";
