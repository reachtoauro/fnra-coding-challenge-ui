    function cloneArray(array, deep) {
        var arr = [];
        if (!array) {
            return arr;
        }
        var i = array.length;
        while (i--) {
            arr[i] = deep ? cloneValue(array[i]) : array[i];
        }
        return arr;
    }
    /**
     * Doesn't clone leaf items
     * @hidden
     */
    function cloneHierarchicalArray(array, childDataKey) {
