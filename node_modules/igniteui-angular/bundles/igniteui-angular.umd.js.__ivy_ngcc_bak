            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(IgxTextSelectionDirective.prototype, "nativeElement", {
            /**
             * Returns the nativeElement of the element where the directive was applied.
             *
             * ```html
             * <input
             *   type="text"
             *   id="firstName"
             *   igxTextSelection>
             * </input>
             * ```
             *
             * ```typescript
