    function flatten(arr) {
        var result = [];
        arr.forEach(function (el) {
            result.push(el);
            if (el.children) {
                var children = Array.isArray(el.children) ? el.children : el.children.toArray();
                result = result.concat(flatten(children));
            }
        });
        return result;
    }
