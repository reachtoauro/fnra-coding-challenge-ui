    };
    /**
     * @hidden
     */
    var IgxTooltipModule = /** @class */ (function () {
        function IgxTooltipModule() {
        }
        return IgxTooltipModule;
    }());
    IgxTooltipModule.decorators = [
        { type: i0.NgModule, args: [{
                    declarations: [IgxTooltipDirective, IgxTooltipTargetDirective],
