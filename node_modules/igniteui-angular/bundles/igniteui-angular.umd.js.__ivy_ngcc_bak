        TransactionEventOrigin["REDO"] = "redo";
        TransactionEventOrigin["CLEAR"] = "clear";
        TransactionEventOrigin["ADD"] = "add";
        TransactionEventOrigin["END"] = "endPending";
    })(exports.TransactionEventOrigin || (exports.TransactionEventOrigin = {}));

    /**
     * @hidden
