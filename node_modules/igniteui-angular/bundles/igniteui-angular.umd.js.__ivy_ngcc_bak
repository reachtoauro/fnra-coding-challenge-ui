        };
        IgxSorting.prototype.getFieldValue = function (obj, key, isDate) {
            if (isDate === void 0) { isDate = false; }
            return isDate ? parseDate(resolveNestedPath(obj, key)) : resolveNestedPath(obj, key);
        };
